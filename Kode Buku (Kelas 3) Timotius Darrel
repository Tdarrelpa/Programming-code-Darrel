package kelas3;

/**
 *
 * @author asus
 */
public class Kelas3 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
         Buku buku1 = new Buku();
        Buku buku2 = new Buku("Pelajaran");
        Buku buku3 = new Buku("Komik", 2007);
        Buku buku4 = new Buku("Majalah", 2015, 1500);
       

        buku1.lihatdata();
        System.out.println();
        buku2.lihatdata();
        System.out.println();
        buku3.lihatdata();
        System.out.println();
        buku4.lihatdata();
        System.out.println();
        
    }
    
}
