    private void btnkeluarActionPerformed(java.awt.event.ActionEvent evt) {                                          
       dispose();
    }                                         

    private void btnprosesActionPerformed(java.awt.event.ActionEvent evt) {                                          
        String a = "", Agama;
        if (radiobtn1.isSelected()) {
     a = "Laki - Laki";
} 
        else if (radiobtn2.isSelected()) {
     a= "Perempuan";
}
        else if (radiobtn3.isSelected()) {
     a= "dan lain-lain";
        } 
    Agama = btnjenisagama.getSelectedItem().toString();
    
    txthasil.setText("Nama\t: " + txtnama.getText()
        + "\n Jenis Kelamin\t: " + a
        + "\n Agama\t: " + Agama
        + "\n Hobi\t: " + txthobi.getText()
        + "\n No. telp\t: " + txtnotelp.getText()
        + "\n Alamat\t: " + txtalamat.getText());
    }                                         

    private void btnjenisagamaActionPerformed(java.awt.event.ActionEvent evt) {                                              
    String Agama;
    Agama = btnjenisagama.getSelectedItem().toString();
    }                                             

    private void btnbersihkanActionPerformed(java.awt.event.ActionEvent evt) {                                             
 txtnama.setText("");
 txthobi.setText("");
 txtnotelp.setText("");
 txtalamat.setText("");
 txthasil.setText("");
    }                                            

