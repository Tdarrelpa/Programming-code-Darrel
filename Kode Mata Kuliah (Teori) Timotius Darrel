package Sub;

import Master.Matakuliah;

/**
 *
 * @author asus
 */
public class Matakuliahteori extends Matakuliah {
    private String Jenisteori;
    
    public Matakuliahteori(){
        super();Jenisteori="tidak tahu";
    }
    public Matakuliahteori(String nm, String dp, int js, double jm, char kt, String jt){
        super(nm,dp,js,jm,kt);Jenisteori=jt;
    }

    /**
     * @return the Jenisteori
     */
    public String getJenisteori() {
        return Jenisteori;
    }

    /**
     * @param Jenisteori the Jenisteori to set
     */
    public void setJenisteori(String Jenisteori) {
        this.Jenisteori = Jenisteori;
    }
    
    @Override
    public void lihatdata(){
        System.out.printf("%25s %20s %15d %15f %15c %18s\n", 
                getNamamatkul(),getDosenpengajar(),getJumlahsks(),getJammasuk(),getKodematkul(),Jenisteori);
    }
}
