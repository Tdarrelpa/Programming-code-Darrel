    private void btnplhnjabatanActionPerformed(java.awt.event.ActionEvent evt) {                                               
  if (btnplhnjabatan.getSelectedItem().equals("--Pilih--")){
    txtgajipokok.setText("");    
    }
    else if (btnplhnjabatan.getSelectedItem().equals("Manajer")){
    txtgajipokok.setText("3000000");    
    }
    else if (btnplhnjabatan.getSelectedItem().equals("Personalia")){
    txtgajipokok.setText("2500000");    
    }
    else if (btnplhnjabatan.getSelectedItem().equals("Customer Service (CS)")){
    txtgajipokok.setText("2000000");    
    }
     else if (btnplhnjabatan.getSelectedItem().equals("karyawan")){
    txtgajipokok.setText("1800000");    
    }
    else if (btnplhnjabatan.getSelectedItem().equals("Satpam (Security)")){
    txtgajipokok.setText("1700000");    
    }
    }                                              

    private void btnplhnjamkerjaActionPerformed(java.awt.event.ActionEvent evt) {                                                
  if (btnplhnjamkerja.getSelectedItem().equals("--Pilih--")){
    txttmbhnuphkerjaprbulan.setText("");    
    }
    else if (btnplhnjamkerja.getSelectedItem().equals("1 jam")){
    txttmbhnuphkerjaprbulan.setText("50000");    
    }
    else if (btnplhnjamkerja.getSelectedItem().equals("2 jam")){
    txttmbhnuphkerjaprbulan.setText("100000");    
    }
    else if (btnplhnjamkerja.getSelectedItem().equals("3 jam")){
    txttmbhnuphkerjaprbulan.setText("150000");    
    }
    else if (btnplhnjamkerja.getSelectedItem().equals("4 jam")){
    txttmbhnuphkerjaprbulan.setText("200000");    
    }
    else if (btnplhnjamkerja.getSelectedItem().equals("5 jam")){
    txttmbhnuphkerjaprbulan.setText("250000");    
    }
    else if (btnplhnjamkerja.getSelectedItem().equals("6 jam")){
    txttmbhnuphkerjaprbulan.setText("300000");
    }
    }                                               

    private void btnplhnbonusjamlemburActionPerformed(java.awt.event.ActionEvent evt) {                                                      
  if (btnplhnbonusjamlembur.getSelectedItem().equals("--Pilih--")){
    txtjlmhuanglembur.setText("");    
    }
    else if (btnplhnbonusjamlembur.getSelectedItem().equals("+0 jam")){
    txtjlmhuanglembur.setText("0");    
    } 
    else if (btnplhnbonusjamlembur.getSelectedItem().equals("+1 jam")){
    txtjlmhuanglembur.setText("1000");    
    }
    else if (btnplhnbonusjamlembur.getSelectedItem().equals("+2 jam")){
    txtjlmhuanglembur.setText("2000");    
    }
    else if (btnplhnbonusjamlembur.getSelectedItem().equals("+3 jam")){
    txtjlmhuanglembur.setText("5000");    
    }
    else if (btnplhnbonusjamlembur.getSelectedItem().equals("+4 jam")){
    txtjlmhuanglembur.setText("10000");    
    }
    else if (btnplhnbonusjamlembur.getSelectedItem().equals("+5 jam")){
    txtjlmhuanglembur.setText("15000");    
    }
    else if (btnplhnbonusjamlembur.getSelectedItem().equals("+6 jam")){
    txtjlmhuanglembur.setText("20000");    
    }
    else if (btnplhnbonusjamlembur.getSelectedItem().equals("+7 jam")){
    txtjlmhuanglembur.setText("25000");    
    }
    else if (btnplhnbonusjamlembur.getSelectedItem().equals("+8 jam")){
    txtjlmhuanglembur.setText("30000");    
    }
    else if (btnplhnbonusjamlembur.getSelectedItem().equals("+9 jam")){
    txtjlmhuanglembur.setText("50000");    
    }
    else if (btnplhnbonusjamlembur.getSelectedItem().equals("+10 jam")){
    txtjlmhuanglembur.setText("100000");    
    }
    }                                                     

    private void btnprosesActionPerformed(java.awt.event.ActionEvent evt) {                                          
    int a, b, c,total;
    
     a = Integer.parseInt(txtgajipokok.getText());
     b = Integer.parseInt(txttmbhnuphkerjaprbulan.getText());
     c = Integer.parseInt(txtjlmhuanglembur.getText());
     total = (a + b + c);
    txtjlmhgaji.setText("RP" +"\n" + String.valueOf(total));
    }                                         

    private void btnbersihkanActionPerformed(java.awt.event.ActionEvent evt) {                                             
    txtgajipokok.setText("");
    txttmbhnuphkerjaprbulan.setText("");
    txtjlmhuanglembur.setText("");
    txtjlmhgaji.setText("");
    }                                            

    private void btnkeluarActionPerformed(java.awt.event.ActionEvent evt) {                                          
    dispose(); 
    }                                         

